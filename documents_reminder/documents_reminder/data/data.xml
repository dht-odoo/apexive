<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Created records for different document types with their respective images -->
    <data>
        <record id="doucument_passport_data" model="reminder.document.type">
            <field name="name">Passport</field>
            <field name="image" type="base64" file="documents_reminder/static/src/img/pass1.jpg"/>
        </record>
        <record id="doucument_visa_data" model="reminder.document.type">
            <field name="name">Visa</field>
            <field name="image" type="base64" file="documents_reminder/static/src/img/visa.jpeg"/>
        </record>
        <record id="doucument_driving_data" model="reminder.document.type">
            <field name="name">Driving Licence</field>
            <field name="image" type="base64" file="documents_reminder/static/src/img/licence.png"/>
        </record>
    </data>

    <!-- Created records for different reminder document type notify-->
    <data>
        <record id="doucument_five_days" model="reminder.document.type.notify">
            <field name="name">Five Days</field>
            <field name="days">5</field>
        </record>
        <record id="doucument_ten_days" model="reminder.document.type.notify">
            <field name="name">Ten Days</field>
            <field name="days">10</field>
        </record>
        <record id="doucument_fifteen_days" model="reminder.document.type.notify">
            <field name="name">Fifteen Days</field>
            <field name="days">15</field>
        </record>
    </data>
</odoo>